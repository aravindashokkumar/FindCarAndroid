<?xml version="1.0" encoding="utf-8"?>
<selector
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="false">
        <shape>
            <gradient android:startColor="#ff406090" android:endColor="#ff406090" android:angle="270.0" />
            <corners android:radius="5.0dip" />
        </shape>
    </item>
    <item android:state_pressed="true">
        <shape>
            <gradient android:startColor="#ff004070" android:endColor="#ff004070" android:angle="270.0" />
            <corners android:radius="5.0dip" />
            <stroke android:width="0.0dip" android:color="#ff004070" />
        </shape>
    </item>
</selector>